/**index.wxss**/
.page.iphone-x {
  padding-bottom: 20rpx;
}

.search-ui {
  background: #12c2e9;
}

.search-form {
  font-size: 90%;
}

.bg-bui {
  background: -webkit-linear-gradient(to right, #6188ef, #98b4f5);
  /* Chrome 10-25, Safari 5.1-6 */
  background: linear-gradient(to right, #6188ef, #98b4f5);
  border: none;
}

/* 首页图标 */
.bg-imgz {
  overflow: hidden;
  content: "";
  width: 100rpx;
  height: 100rpx;
  background-repeat: no-repeat;
  background-size: 100rpx 100rpx;
  margin: 0 auto;
  background-image: url(data:image/png;base64,);
}

.bg-imgd {
  overflow: hidden;
  content: "";
  width: 100rpx;
  height: 100rpx;
  background-repeat: no-repeat;
  background-size: 100rpx 100rpx;
  margin: 0 auto;
  background-image: url(data:image/png;base64,)
}

.bg-imgh {
  overflow: hidden;
  content: "";
  width: 100rpx;
  height: 100rpx;
  background-repeat: no-repeat;
  background-size: 100rpx 100rpx;
  margin: 0 auto;
  background-image: url(data:image/png;base64,)
}

.bg-imge {
  overflow: hidden;
  content: "";
  width: 100rpx;
  height: 100rpx;
  background-repeat: no-repeat;
  background-size: 100rpx 100rpx;
  margin: 0 auto;
  background-image: url(data:image/png;base64,)
}

.auth-btn {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: transparent;
  padding: 0;
  text-align: left;
  line-height: 120rpx;
  color: #3A3A3A;
}

.banner-bg {
  position: absolute;
  width: 100vw;
  bottom: 0;
  left: 0;
}

/* 内容 */
.tab-content {
  background: #ffffff;
}

.banner::after {
  content: "";
  display: block;
  height: 80rpx;
  width: 100vw;
  position: absolute;
  left: 0;
  bottom: 0;
  background-image: linear-gradient(180deg, rgba(255, 255, 255, 0.00) 0%, #F9F9F9 100%);
  pointer-events: none;
  z-index: -1;
}

.banner-info {
  position: absolute;
  left: 0;
  right: 0;
  margin: 0 40rpx;
}

.banner-title {
  font-weight: bold;
  font-size: 58rpx;
  color: #000;
  line-height: 68rpx;
}

/* 置顶文章 */
.tower-swiper .tower-item {
  transform: scale(calc(0.5 + var(--index) / 10));
  margin-left: calc(var(--left) * 100rpx - 150rpx);
  z-index: var(--index);
}

.screen-swiper {
  text-align: center;
  padding: 12px;
  background: #FFFFFF;
}

.banner-title_text {
  display: inline-block;
  vertical-align: middle;
}

.banner-avatar {
  position: relative;
  vertical-align: middle;
  float: right;
}

.avatar {
  width: 72rpx;
  height: 72rpx;
  border: 5rpx solid #fff;
  border-radius: 50%;
  background-size: cover;
}

.default-avatar {
  width: 72rpx;
  height: 72rpx;
  border: 5rpx solid #fff;
  border-radius: 50%;
  background-image: url(https://cloud-minapp-16269.cloud.ifanrusercontent.com/default-avator.jpg);
  background-size: cover;
}

.unread-message {
  display: block;
  position: absolute;
  right: 0;
  top: 0;
  pointer-events: none;
}

.banner-brief {
  font-size: 28rpx;
  color: #121212;
  line-height: 40rpx;
  margin-top: 20rpx;
  letter-spacing: 4rpx;
}

.index-search {
  position: absolute;
  bottom: -20rpx;
  left: 40rpx;
  right: 40rpx;
  border-radius: 44rpx;
  box-shadow: 0 4rpx 20rpx 2rpx #FBEBD4;
}


.index {
  padding: 10rpx 36rpx;
}

.top_text {
  width: 100%;
  text-align: center;
  color: #AAA;
  font-size: 24rpx;
}

page::-webkit-scrollbar {
  display: none;
}

.slide_box {
  width: 678rpx;
  height: 280rpx;
  margin: 0 auto;
  box-shadow: 0rpx 2rpx 14rpx 0rpx rgba(38, 38, 38, 0.1);
}

.slide_image {
  width: 678rpx;
  height: 280rpx;
  border-radius: 4rpx;
}

.dots {
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 20rpx;
}

.dot {
  width: 20rpx;
  height: 4rpx;
  background: rgba(221, 221, 221, 1);
  margin-right: 20rpx;
}

.active {
  width: 60rpx;
  height: 4rpx;
  background: rgba(38, 38, 38, 1);
}

.category {
  padding: 30rpx 10rpx;
}

.index_label {
  position: relative;
  margin-top: 80rpx;
}

.index_label_title {
  font-size: 45rpx;
  color: #262626;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

.index_label_bg {
  width: 160rpx;
  height: 12rpx;
  background: linear-gradient(270deg, rgba(249, 228, 135, 0) 0%, rgba(241, 197, 79, 1) 100%);
  border-radius: 11rpx;
  position: absolute;
  top: 48rpx;
  left: 0;
}

.index_label_more {
  position: absolute;
  right: 0;
  top: -14rpx;
  color: #888888;
  font-size: 28rpx;
  line-height: 48rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.wz-ui {
  border-bottom: 2px solid #f7f7f7;
}

.margin_right_30 {
  margin-right: 30rpx;
}

.margin_right_0 {
  margin-right: 0;
}

.index_article_box {
  margin-bottom: 38rpx;
  padding-bottom: 38rpx;
  border-bottom: 1px dotted #c5c5c5;
}

.index_article_figures {
  position: relative;
  display: block;
  width: 100%;
  max-height: 56.25vh;
}

.index_article_figure {
  position: relative;
  display: block;
  width: 100%;
  height: 0;
  padding: 0;
  padding-bottom: 56.25%;
  overflow: hidden;
}

.index_article_covers {
  width: 100%;
  height: 380rpx;
  border-radius: 15rpx;
}

.index_article_cover {
  width: 100%;
  height: 380rpx;
  border-radius: 15rpx;
}

.index_article {
  margin-top: 210rpx;
}

.index_article_title {
  width: 100%;
  font-size: 110%;
  font-weight: 500;
  color: rgba(38, 38, 38, 1);
  line-height: 50rpx;
  margin-top: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.index_article_desc {
  color: #888888;
  margin-top: 16rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-wrap: break-word;
  overflow: hidden;
}

.index_article_during {
  position: absolute;
  top: 326rpx;
  left: 16rpx;
  z-index: 1;
  line-height: 40rpx;
  background: rgba(38, 38, 38, 1);
  border-radius: 8rpx;
  opacity: 0.9;
  padding: 2rpx 10rpx;
  text-align: center;
}

.index_article_during text {
  font-size: 80%;
  color: #fff;
}

.last_text {
  width: 100%;
  text-align: center;
  color: #AAA;
  font-size: 24rpx;
  margin: 90rpx 0 30rpx;
}

.dialog .t1 {
  color: #262626;
  font-size: 36rpx;
  width: 100%;
  text-align: center;
  margin: 70rpx auto 50rpx;
}

.dialog .t2 {
  color: #888;
  font-size: 32rpx;
  width: 100%;
  text-align: center;
}

.dialog .t3 {
  width: 500rpx;
  height: 174rpx;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
}

.dialog .t4 {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  padding: 0 30rpx;
}

.dialog .t4 .cancel {
  width: 240rpx;
  height: 70rpx;
  background: #fff;
  border: 1rpx solid #F1C54F;
  color: #F1C54F;
  text-align: center;
  line-height: 70rpx;
}

.dialog .t4 .go_set {
  width: 240rpx;
  height: 70rpx;
  background: rgba(241, 197, 79, 1);
  color: #fff;
  text-align: center;
  line-height: 70rpx;
}

.index-search {
  display: flex;
  flex-flow: row;
  background: #FFFFFF;
  box-shadow: 0 6rpx 8rpx 0 #F5F5F5;
  border-radius: 44rpx;
  height: 88rpx;
  justify-content: center;
  align-items: center;
  padding: 24rpx;
  box-sizing: border-box;
}

.search-icon {
  flex-grow: 0;
  flex-shrink: 0;
  width: 40rpx;
  height: 40rpx;
  background-image: url("https://cloud-minapp-16269.cloud.ifanrusercontent.com/product-search.svg");
  background-size: contain;
}

.search-input {
  flex-grow: 1;
  flex-shrink: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 20rpx;
  font-size: 26rpx;
  color: #A8A8A8;
}

input.search-input {
  color: #222222;
}

search-input-placeholder {
  color: #A8A8A8;
}

.close-btn {
  width: 36rpx;
  height: 36rpx;
  margin-left: 24rpx;
  border-radius: 50%;
  opacity: 0.4;
}